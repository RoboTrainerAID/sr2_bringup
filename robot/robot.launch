<?xml version="1.0"?>
<launch>

	<arg name="robot_env" default="$(optenv ROBOT_ENV ipr-roboterhalle)"/>
	<arg name="pkg_env_config" default="$(find cob_default_env_config)"/>

	<include file="$(find cob_bringup)/robots/raw3-5.xml" >
		<arg name="pc1" value="i60sr2"/>
	</include>
    
    <!--ERROR here: Ignoring transform for child_frame_id "led_rectangle_link" from authority "unknown_publisher" because of an invalid quaternion in the transform-->
	<include file="$(find iirob_led)/launch/iirob_led_rectangle.launch"/>

	<!-- upload default configuration parameters -->
	<include file="$(arg pkg_env_config)/upload_navigation_goals.launch">
		<arg name="robot_env" value="$(arg robot_env)" />
	</include>

</launch>
